{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "quotemark": [true, "single"],
        "trailing-comma": [ false ],
        "no-console": [false],
        "object-literal-sort-keys": false,
        "object-literal-key-quotes": false,
        "no-shadowed-variable": [
          true,
          {
            "class": true,
            "enum": true,
            "function": true,
            "interface": false,
            "namespace": true,
            "typeAlias": false,
            "typeParameter": false
          }
        ]
    },
    "rulesDirectory": []
}
